{
  "name": "portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3000",
    "build": "run-s build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint --dir src --ignore-path .prettierignore --max-warnings 0",
    "lint:typecheck": "tsc --pretty --noEmit",
    "lint:stylelint": "stylelint --ignore-path .gitignore './src/**/*.css'",
    "lint:prettier": "prettier --check .",
    "format": "run-s format:*",
    "format:eslint": "pnpm lint:next --fix",
    "format:stylelint": "pnpm lint:stylelint --fix",
    "format:prettier": "pnpm lint:prettier --write",
    "test": "run-s test:*",
    "test:component": "vitest run",
    "test:e2e": "playwright test -c playwright.config.e2e.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@next/font": "13.2.4",
    "@react-spring/three": "^9.7.1",
    "@react-three/drei": "^9.57.3",
    "@react-three/fiber": "^8.12.0",
    "@sendgrid/mail": "^7.7.0",
    "@slack/web-api": "^6.8.1",
    "@tanstack/query-core": "^4.26.1",
    "@tanstack/react-query": "^4.26.1",
    "@trpc/client": "^10.16.0",
    "@trpc/react-query": "^10.16.0",
    "@trpc/server": "^10.16.0",
    "async_hooks": "^1.0.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.5",
    "superjson": "^1.12.2",
    "three": "^0.150.1",
    "wretch": "^2.5.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@tailwindcss/container-queries": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/three": "^0.149.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.0.3",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-plugin-testing-library": "^5.10.2",
    "happy-dom": "^8.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^30.0.1",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.5",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.2"
  },
  "volta": {
    "node": "18.12.1"
  }
}
